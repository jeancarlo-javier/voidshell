{
  "name": "voidshell",
  "version": "0.1.0",
  "description": "VoidShell â€“ minimal terminal React component as a reusable package.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/voidshell.cjs.js",
  "module": "./dist/voidshell.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/voidshell.es.js",
      "require": "./dist/voidshell.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "typescript": "^5.5.0",
    "vite": "^5.4.0",
    "@vitejs/plugin-react": "^4.3.0",
    "vite-plugin-dts": "^4.2.3"
  },
  "scripts": {
    "dev": "vite",
    "build:lib": "vite build",
    "build:demo": "vite build --config vite.demo.config.ts",
    "build": "npm run build:lib && npm run build:demo",
    "preview": "vite preview --config vite.demo.config.ts --outDir demo-dist"
  }
}
